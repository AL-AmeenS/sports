
<section>
    <div class="container" style="margin-top: 5rem;">
        <table class="table mt-5">
  <thead>
      
    <tr>
            <th scope="col">No</th>
            <th scope="col">Event Name</th>
            <th scope="col">Date</th>
            <th scope="col">Venue</th>
            <th scope="col">Team Strength</th>
            <th scope="col">No of Participants</th>
            <th scope="col">Description</th>
            <th scope="col">Images</th>
            <th scope="col">Delete</th>
          </tr>
  </thead>
  <tbody>
      {{#each event}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.eventname}}</td>
      <td>{{this.date}}</td>
      <td>{{this.venue}}</td>
      <td>{{this.teamstrength}}</td>
      <td>{{this.noparticipants}}</td>
      <td>{{this.description}}</td>
      <td><img style="width:50px ;height:50px" src="/Event_images/{{this.imagename}}"></td>
       <td>
         <a href="/admin/deleteproduct/{{this._id}}" class="btn btn-danger" onclick="return confirm('are you sure? {{this.name}}')">Delete</a>
      </td>
    </tr>
    {{/each}}
    
  </tbody>
</table>
    </div>
</section>